{
  "expo": {
    "name": "剧词记",
    "slug": "dramaword-mobile",
    "version": "1.58.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "dramaword",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-localization"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tannibunni.dramawordmobile",
      "buildNumber": "26",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIFileSharingEnabled": false,
        "LSSupportsOpeningDocumentsInPlace": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tannibunni.dramawordmobile"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "c25a11c6-d764-4f2a-a81d-92692409b5fa"
      }
    },
    "owner": "tannibunni"
  }
}
